<?xml version="1.0"?>
<odoo>
    <data>
        <record id="account_journal_inherit_view_form" model="ir.ui.view">
            <field name="name">account_journal_inherit_view_form</field>
            <field name="model">account.journal</field>
            <field name="inherit_id" ref="account.view_account_journal_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='type']" position="after">
                    <field name="x_corporate_funds"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
