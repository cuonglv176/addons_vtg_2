            <xml-property name="queryText"><![CDATA[SELECT hd.name                                                                                     AS bo_phan1,
       he.x_employee_code                                                                          AS ma_nv,
       he.job_priority                                                                             AS stt,
       hd.name                                                                                     AS bo_phan,
       REGEXP_REPLACE(he.name, '( [^ ]+)$', '')                                                    AS ho,
       REGEXP_REPLACE(he.name, '.+[\s]', '')                                                       AS ten,
       he.birthday                                                                                 AS ngay_sinh,
       he.date_join_company                                                                        AS ngay_vao_cong_ty,
       (SELECT SUM(number_of_days)
        FROM hr_leave_allocation hla
        WHERE hla.employee_id = he.id
          AND hla.years = EXTRACT(YEAR FROM month.month)::VARCHAR(255)
          AND SPLIT_PART(hla.month, '/', 1)::INT <= EXTRACT(MONTH FROM ?::TIMESTAMP))              AS quy_phep,
       (SELECT COALESCE(SUM(number_of_days), 0)
        FROM hr_leave_allocation hla
        WHERE hla.employee_id = he.id
          AND hla.month = TO_CHAR(month.month, 'MM/YYYY'))                                         AS cap_phep_thang,
       ''                                                                                          AS phep_doc_hai,
       EXTRACT(YEAR FROM AGE(?::TIMESTAMP, he.date_join_company))                                  AS nam_cong_tac,
       CASE
           WHEN EXTRACT(YEAR FROM AGE(?::TIMESTAMP, he.date_join_company)) >= 10 THEN 2
           WHEN EXTRACT(YEAR FROM AGE(?::TIMESTAMP, he.date_join_company)) >= 5 THEN 1
           ELSE 0 END                                                                              AS phep_tham_nien,
       CONCAT('Tháng ', TO_CHAR(month.month, 'MM'))                                                AS thang,
       (SELECT COALESCE(SUM(CASE
                                WHEN hll.type = 'full' THEN 1
                                ELSE 0.5 END), 0)
        FROM hr_leave_line hll
                 JOIN hr_leave hl ON hll.leave_id = hl.id,
             hr_leave_type hlt
        WHERE hl.employee_id = he.id
          AND hl.holiday_status_id = hlt.id
          AND hlt.code = 'P'
          AND hl.is_end_workflow IS TRUE
          AND EXTRACT(MONTH FROM hll.date) = EXTRACT(MONTH FROM month.month))          AS phep_sd,
       (SELECT COALESCE(SUM(CASE
                                WHEN hll.type = 'full' THEN 1
                                ELSE 0.5 END), 0)
        FROM hr_leave_line hll
                 JOIN hr_leave hl ON hll.leave_id = hl.id,
             hr_leave_type hlt
        WHERE hl.employee_id = he.id
          AND hl.holiday_status_id = hlt.id
          AND hlt.code = 'P'
          AND hl.is_end_workflow IS TRUE
          AND EXTRACT(MONTH FROM hl.request_date_from) <= EXTRACT(MONTH FROM ?::TIMESTAMP))        AS tong_phep_sd,
       STRING_AGG(CASE
                      WHEN ab.type = 'full' AND
                           EXTRACT(MONTH FROM ab.date) = EXTRACT(MONTH FROM month.month)
                          THEN EXTRACT(DAY FROM ab.date)::VARCHAR(255)
                      WHEN ab.type = 'before' AND
                           EXTRACT(MONTH FROM ab.date) = EXTRACT(MONTH FROM month.month)
                          THEN CONCAT('S', EXTRACT(DAY FROM ab.date)::VARCHAR(255))
                      WHEN ab.type = 'after' AND
                           EXTRACT(MONTH FROM ab.date) = EXTRACT(MONTH FROM month.month)
                          THEN CONCAT('C', EXTRACT(DAY FROM ab.date)::VARCHAR(255)) END,
                  ', ')                                                                            AS ngay_nghi,
       ((SELECT SUM(number_of_days)
         FROM hr_leave_allocation hla
         WHERE hla.employee_id = he.id
           AND hla.years = EXTRACT(YEAR FROM month.month)::VARCHAR(255)
           AND SPLIT_PART(hla.month, '/', 1)::INT <= EXTRACT(MONTH FROM ?::TIMESTAMP))
           - (SELECT COALESCE(SUM(hl.number_of_days), 0)
              FROM hr_leave hl,
                   hr_leave_type hlt
              WHERE hl.employee_id = he.id
                AND hl.holiday_status_id = hlt.id
                AND hlt.code = 'P'
                AND hl.is_end_workflow IS TRUE
                AND EXTRACT(YEAR FROM hl.request_date_from) = EXTRACT(YEAR FROM month.month)
                AND EXTRACT(MONTH FROM hl.request_date_from) <= EXTRACT(MONTH FROM ?::TIMESTAMP))) AS phep_con_lai
FROM hr_employee he
         LEFT JOIN (SELECT hl.employee_id, hll.type, hll.date, hl.request_date_from
                    FROM hr_leave_line hll
                             left JOIN hr_leave hl ON hll.leave_id = hl.id,
                         hr_leave_type hlt
                    WHERE hl.holiday_status_id = hlt.id
                      AND hlt.code = 'P'
                      AND hl.is_end_workflow IS TRUE) ab
                   ON he.id = ab.employee_id
         JOIN hr_department hd ON he.department_id = hd.id,
     GENERATE_SERIES(DATE_TRUNC('year', ?::TIMESTAMP), ?, '1 month') month
WHERE (he.department_id = ANY (STRING_TO_ARRAY(?, ',')::INTEGER[]) OR ? = '0')
  AND he.date_join_company <= ?
  AND (he.date_leave is null OR he.date_leave >= ?)
GROUP BY hd.name, he.x_employee_code, he.job_priority, he.name, he.birthday, he.date_join_company, he.id, month.month;]]></xml-property>
